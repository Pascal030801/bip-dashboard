{
    // "extends": [
    //     "eslint:recommended",
    //     "plugin:react/recommended"
    // ],
    "rules": {},
    "root": true,
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "env": {
        "commonjs": true,
        "node": true,
        "browser": true,
        "es6": true,
        "jest": true
    },
    "plugins": [
        "react",
        "import",
        "react-hooks"
    ],
    "ignorePatterns": [
        "node_modules/"
    ],
    "settings": {
        "react": {
            "version": "latest" // "detect" automatically picks the version you have installed.
        }
    }
}